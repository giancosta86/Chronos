event start {
    create sumOperands;

    x(sumOperands) := readDouble("Please, enter a double X: ");
    y(sumOperands) := readInt("Please, enter an int Y: ");

    z := x(sumOperands) + y(sumOperands);
    println "The result is: " + z;

    destroy sumOperands;


    variablePromptString := "Please, enter a boolean: ";

    myBool := readBool(variablePromptString);
    println myBool;

    create book;

    variableIntegerInput := 56;
    title(book) := readString(variableIntegerInput);
    println title(book);

    destroy book;
}
